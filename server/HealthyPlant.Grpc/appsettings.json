{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "LoggerOptions": {
    "ExpectElapsedLessThanMs": 2000,
    "ExpectedDbDurationLessThanMs": 500
  }
}
